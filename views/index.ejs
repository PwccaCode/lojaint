            <a href="novoprod">
                <span class="text-gradient">Novo Produto <i class="fa-solid fa-plus"></i></span>
            </a>
           </div>

           <!--Form-->
           <form action="/" method="POST">
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        <th>ID</th>
                        <th>Nome</th>
                        <th>Fabricante</th>
                        <th>Categoria</th>
                        <th>Quantidade</th>
                        <th>Valor</th>
                        <th>Ação</th>
                    </tr>
                </thead>
                <tbody>
                    <%-include('include/_show') %>
                </tbody>
            </table>
           </form>
        </div>
