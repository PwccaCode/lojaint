    <script>
            function popular()
            {
                var a=document.getElementById("fabric").value;
                if(a==="nestle")
                {
                    var arr=["nestle1","nestle2"];
                }
                else if(a==="varig")
                {
                    var arr=["aviao1","aviao2"];
                }

                var string="";

                for(i=0;i<arr.length;i++)
                {
                    string=string+"<option value="+arr[i]+">"+arr[i]+"</option>";
                }
                document.getElementById("categ").innerHTML=string;
            }
        </script>
    <style>
      .wf-force-outline-none[tabindex="-1"]:focus {
        outline: none;
