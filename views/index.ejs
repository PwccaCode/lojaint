  <body>
    <div class="w-row">
      <div class="column w-col w-col-4">
        <h3>Cadastrar produto</h3>
        <div class="w-form">
          <form
            id="wf-form-produtoadd"
            name="wf-form-produtoadd"
            data-name="produtoadd"
            aria-label="produtoadd"
             method="post" action="/"
          >
            <label for="nomeprod">Nome produto</label
            ><input
              type="text"
              name="nome"
              data-name="nome"
              placeholder=""
              id="nome"
            /><label for="fabricante">fabricante </label
            ><select
              id="fabric"
              name="fabric"
              data-name="fabric"
              required=""
              class="w-select"
              onchange="popular()"
            >
              <option value="nestle">nestle</option>
              <option value="varig">varig</option>
            </select>
            <div class="text-block-2">categoria</div>
            <select
              data-name="categ"
              name="categ"
              id="categ"
              class="w-select"
            >
            </select>
            <div class="text-block-3">quantidade</div>
            <input
              type="text"
              class="w-input"
              maxlength="256"
              name="quant"
              data-name="quant"
              placeholder=""
              id="quant"
            />
            <div class="text-block-4">valor unidade</div>
            <input
              type="text"
              class="w-input"
              maxlength="256"
              name="val"
              data-name="val"
              placeholder=""
              id="val"
              required=""
            /><input
              type="submit"
              value="Enviar"
              class="w-button"
            />
          </form>
          </div>
        </div>
      </div>
    </div>
    <div class="column-2 w-col w-col-8">
<table class="table">
  <thead>
    <tr>
      <th scope="col">Nome</th>
      <th scope="col">Fabricante</th>
      <th scope="col">Categoria</th>
      <th scope="col">Quantidade</th>
      <th scope="col">Valor</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <%listaProdu.forEach(loja => {%>
      <th scope="row"><%= loja._nome %></th>
      <td><%= loja._fabric %></td>
      <td><%= loja._categ %></td>
      <td><%= loja._quant %></td>
      <td><%= loja._val %></td>
    </tr>
    <%})%>
  </tbody>
</table>


